<table>
  <tr>
    <th>Name</th>
    <th>Category</th>
    <th>Date</th>
    <th>Time</th>

    <th>Location</th>
    <th>Description</th>
        <th>Notes</th>

        <th></th>
        <th></th>
        <th></th>
  </tr>

  <% @events.each do |event| %>
  <tr>
    <td><%= event.name %></td>
    <td><%= event.category %></td>
    <td><%= event.date %></td>
    <td><%= event.time %></td>

    <td><%= event.location %></td>
    <td><%= event.description %></td>
    <td><%= event.notes %></td>

    <!--     :category, :date, :description, :location, :name, :notes, :time, :wedding_id, :complete -->
    <td><%= link_to 'Show', event %></td>
    <td><%= link_to 'Edit', edit_event_path(event) %></td>
    <td><%= link_to 'Destroy', event, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
  <% end %>
</table>

